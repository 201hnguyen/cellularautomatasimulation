<?xml version="1.0" encoding="UTF-8"?>
<simulation simulationType = "Predator and Prey">

    <colors>
        <water>LightBlue</water>
        <fish>Yellow</fish>
        <shark>Red</shark>
    </colors>

    <num_rows>80</num_rows>
    <num_columns>60</num_columns>
    <num_neighbors>4</num_neighbors>

    <neighbor_configuration> -1 0 2 0 -1 3 1 0 2</neighbor_configuration>

    <parameters>
        <parameter0>starting_shark_energy:3</parameter0>
        <parameter1>shark_energy_gain:2</parameter1>
        <parameter2>shark_energy_loss:1</parameter2>
        <parameter3>shark_reproducibility_threshold:10</parameter3>
        <parameter4>fish_reproducibility_threshold:3</parameter4>
    </parameters>

    <initial_rectangular_grid>
        1 3 2 1
    </initial_rectangular_grid>>
</simulation>